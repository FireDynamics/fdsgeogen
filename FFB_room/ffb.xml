<fds>

    <para dim="vec" var="tmpa" list="np.linspace(10.0,30.0,20)" />
    <para dim="vec" var="zul_vel" list="np.linspace(0.22,0.32,20)" />
    <para dim="vec" var="soot_yield" list="np.linspace(0.01,0.05,20)" />
    <para dim="vec" var="hrrpua" list="np.linspace(640.0,960.0,20)" />
    <para dim="vec" var="door_y2" list="np.linspace(6.3,8.0,20)" />
    <para dim="vec" var="window_x1" list="np.linspace(3.0,5.5,20)" />
    
    <info chid="'ffb_room'" outfile="'ffb_room_%03d.fds'%para_id"/>

    <dump str="'!! ################'" />
    <dump str="'!! TMPA       = %f'%tmpa" />
    <dump str="'!! VEL        = %f'%zul_vel" />
    <dump str="'!! SOOT_YIELD = %f'%soot_yield" />
    <dump str="'!! DOOR_Y     = %f'%door_y2" />
    <dump str="'!! WINDOW_X   = %f'%window_x1" />
    <dump str="'!! ################'" />
    

    <input str="'MISC RESTART=.FALSE., TMPA=%f'%tmpa" />

    <surf   id="'Zuluft'"
            color="'BLUE'"
            vel="-zul_vel" />

    <reac   id="'PROPAN'"
            heat_of_combustion="46400."
            soot_yield="soot_yield"
            C="3.0"
            H="8.0" />

    <surf   id="'Gasbrenner'"
            hrrpua="hrrpua"
            color="'RED'" />
            
    <hole   x1="2.3" x2="2.6"
            y1="6.3" y2="door_y2"
            z1="0.0" z2="0.9" />
            
    <hole   x1="window_x1" x2="5.5"
            y1="2.3" y2="2.6"
            z1="1.8" z2="2.6" />

    <dump   file="ffb.fdsinput" />

</fds>
