<fds>

	<info chid="'bottleneck'" outfile="'bottleneck.fds'" title="'bottleneck'"/>
	
	<!-- TIME -->
    <input text="TIME T_END=0.0" />

	<!-- room discretisation and mesh -->
	<var delta = "0.1" />
	
	<var x1 = "0.0"  x2 = "10"/>
    <var y1 = "0.0"  y2 = "10"/>
    <var z1 = "0.0"  z2 = "3"/>

    <bounded_room />
    <evac_mesh />

    <boundary x="open" y="open" zmax="open"/>

    <var wt="0.2" />

    <var width_bn="2.0" />
    <var length_bn="4" />
    <var width_room="10.0" />
    <var length_room="6.0" />

    <obst id="'wall_1'" xb="width_room*0.5+width_bn*0.5,width_room*0.5+width_bn*0.5+wt,length_room,length_room+length_bn,0,2" color="'GRAY 60'" />
    <obst id="'wall_2'" xb="width_room*0.5+width_bn*0.5, width_room, length_room, length_room+wt, 0, 2" color="'GRAY 60'" />
    <obst id="'wall_3'" xb="0+width_room-wt,0+width_room,0,length_room+wt,0,2" color="'GRAY 60'" />
    <obst id="'wall_4'" xb="0, 0+width_room, 0, 0+wt, 0, 2" color="'GRAY 60'" />
    <obst id="'wall_5'" xb="0,0+wt,0,length_room,0,2" color="'GRAY 60'" />
    <obst id="'wall_6'" xb="0,width_room*0.5-width_bn*0.5, length_room, length_room+wt, 0, 2" color="'GRAY 60'" />
	<obst id="'wall_7'" xb="width_room*0.5-width_bn*0.5,width_room*0.5-width_bn*0.5+wt,length_room,length_room+length_bn,0,2" color="'GRAY 60'" />

    <!-- evac parameter -->
    <input text="MISC EVACUATION_DRILL=.TRUE."/>

	<exit id="'EXIT'" ior="+2" xyz="5, 9.8, 2" xb="4.2, 6.2, 10, 10, 0.4, 1.6" />

	<evac id="'EVAC_Adult_1'" number_initial_persons="10" xb="0,10,0,6,0.4,1.6" agent_type="1" pers_id="'ADULT'" />

	<pers id="'ADULT'" avatar_color="'BLUE'" color_method="3" default_properties="'ADULT'" det_mean="0" pre_mean="0" l_non_sp="0.3" dens_init="3" />

    <!-- output -->
    <slcf pbz="1.0" quantity="'VELOCITY'" vector="True" evacuation="True" />

</fds>
