<fds>

    <var epsilon      = "#EPSILON#" /> 
    <var conductivity = "#K#" />
    <var capacity     = "#C_P#" />
    <var rad_flux     = "#RAD_FLUX#" />
 
  <!--   
    <var epsilon      = "0.1" /> 
    <var conductivity = "0.2" />
    <var capacity     = "0.3" />
    <var rad_flux     = "0.4" />
  --> 

    <var delta    = "0.05" />

    <var chid="'cone_test'" outfile="'cone_test.fds'" />
    
    <info chid="chid" outfile="outfile"/>

    <dump str="'!! ################'" />
    <dump str="'!! EPSILON  = %f'%epsilon" />
    <dump str="'!! K        = %f'%conductivity" />
    <dump str="'!! C_P      = %f'%capacity" />
    <dump str="'!! RAD_FLUX = %f'%rad_flux" />
    <dump str="'!! ################'" />

    <input text="TIME T_END=25.0" />
    <input text="MISC RESTART=.FALSE." />
    <input text="DUMP DT_BNDF=0.1, DT_RESTART=300.0" />


    <var xmin = "0.0"  ymin = "0.0"  zmin = "0.0" />
    <var xmax = "0.2"  ymax = "0.2"  zmax = "0.2" />    
    <var nx = "int(div235((xmax - xmin) / delta))" />
    <var ny = "int(div235((ymax - ymin) / delta))" />
    <var nz = "int(div235((zmax - zmin) / delta))" />
    
    <mesh />
    <boundary x="open" y="open" zmax="open" />
    

    <surf id="'FoamSurfHeatFlux'"
	  matl_id="'FOAM'"
	  matl_mass_fraction="1.0"
	  thickness="0.05"
	  external_flux="rad_flux"
	  backing="'INSULATED'" />

    <matl id="'FOAM'"
        specific_heat="capacity"
        conductivity="conductivity"
        emissivity="epsilon"
        density="32.0"
        heat_of_combustion="24900"
        n_reactions="2"
        a="1:2; 2.43e6, 4.30e11"
        e="1:2; 102e3, 177e3"
        n_s="1:2; 0.0, 0.0"
        heat_of_reaction="1:2; 1059, 295"
        spec_id="1,1:2; 'METHANE','METHANE'"
        nu_spec="1,1:2; 1.0, 1.0" />

    <var xymin="0.05" xymax="0.15" />

    <reac id="'METHANE'" fuel="'METHANE'" />

    <surf id="'FoamSurf'"
        rgb="255,204,0"
        matl_id="'FOAM'"
        matl_mass_fraction="1.0"
        thickness="0.1" />

    <obst xb="xymin, xymax, xymin, xymax, 0.0, 0.05" 
        surf_ids="'FoamSurfHeatFlux','FoamSurf','FoamSurf'" />

    <input text="BNDF QUANTITY='BURNING RATE'" />
    <input text="BNDF QUANTITY='WALL TEMPERATURE'" />

</fds>
